<html lang="en">

<head>
    <title>Contact</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="css/main.css">
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="main.js"></script>
</head>

<body>
    <% include ../helpers/navbar %>
    <% include ../helpers/sidenav %>

    <div class="column-main"><input name="form_key" type="hidden" value="wMZ2wRfEPcd3wmOh">
        <div id="authenticationPopup" data-bind="scope:'authenticationPopup'" style="display: none;">
            <script>
                window.authenticationPopup = { "autocomplete": "off", "customerRegisterUrl": "https:\/\/www.kickscrew.com\/customer\/account\/create\/", "customerForgotPasswordUrl": "https:\/\/www.kickscrew.com\/customer\/account\/forgotpassword\/", "baseUrl": "https:\/\/www.kickscrew.com\/" };
            </script>
            <!-- ko template: getTemplate() -->
            <!-- /ko -->
        </div>
    
        <div id="monkey_campaign" style="display:none;"></div>

        <div class="contact-office">
            <h4>Our Store</h4>
            <p class="contact-shop"><i class="fas fa-store" style="font-size: 16px; color: #676767;"></i>&nbsp;&nbsp; FortunesWear
                Store</p>
            <p class="contact-phone"><i class="fa fa-fw fa-phone"></i>+254702425821&nbsp;&nbsp;&nbsp;<a href="https://api.whatsapp.com/send?phone=+254702425821"
                    target="_blank"><i class="fab fa-whatsapp-square" style="font-size: 18px; color: #00e676;"></i></a></p>
            <p class="contact-address"><i class="fa fa-fw fa-map-marker"></i>Gala Place, Shop 1621-22, 56 Dundas St, Mong Kok,<br> KLN, Hong Kong</p>
            <div id="googlemap" style="padding-right: 1vw;"><iframe style="border: 0; width: 100%; height: 210px;"
                    src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d59048.95101693879!2d114.17116349360286!3d22.332495653365715!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x340401ebe88946ed%3A0x108a0d09d21dc313!2sKicksCrew+Store!5e0!3m2!1szh-TW!2shk!4v1546597186847"
                    width="320" height="240" frameborder="0" allowfullscreen=""></iframe></div>
            <br>
            <h4>Our Office</h4>
            <p><i class="fas fa-building" style="font-size: 16px; color: #676767;"></i>&nbsp;&nbsp;&nbsp; FortunesWear Company Limited</p>
            <p class="contact-email">cs@fortuneswear.com</p>
            <p class="contact-phone">+254702425821</p>
            <p class="contact-address">Flat D, 13/F, Southeast Industrial Building,<br> 611-619 Castle Peak Road, Tsuen Wan,
                <br> NT, Hong Kong</p>
        </div>

        <form class="form-contact" action="https://fortuneswear.herokuapp.com/contact/index/post/" id="contact-form" method="post"
            data-hasrequired="* Required Fields" novalidate="novalidate">
            <h4>Contact Us</h4>
            <fieldset class="fieldset" style="width: 100%;">
                <div class="field name required">
                    <label class="label" for="name"><span>Name</span></label>
                    <div class="control">
                        <input name="name" id="name" title="Name" value="" class="input-text" type="text"
                            data-validate="{required:true}" aria-required="true" style="width: 100%;">
                    </div>
                </div>
                <div class="field email required">
                    <label class="label" for="email"><span>Email</span></label>
                    <div class="control">
                        <input name="email" id="contact-email" title="Email" value="" class="input-text" type="email"
                            data-validate="{required:true, 'validate-email':true}" aria-required="true" style="width: 100%;">
                    </div>
                </div>
                <div class="field telephone">
                    <label class="label" for="telephone"><span>Phone Number</span></label>
                    <div class="control">
                        <input name="telephone" id="telephone" title="Phone Number" value="" class="input-text" type="text" style="width: 100%;">
                    </div>
                </div>
                <div class="field comment required">
                    <label class="label" for="comment"><span>What’s on your mind?</span></label>
                    <div class="control">
                        <textarea name="comment" id="comment" title="What’s on your mind?" class="input-text" cols="5"
                            rows="3" data-validate="{required:true}" aria-required="true" style="width: 100%;"></textarea>
                    </div>
                </div>
                <div class="field-recaptcha" id="msp-recaptcha-d84b65fc916e7398083ea5f17089d8f1-container"
                    data-bind="scope:'msp-recaptcha-d84b65fc916e7398083ea5f17089d8f1'">
                    <!-- ko template: getTemplate() -->
                    <!-- ko if: (getIsVisible()) -->
                    <div data-bind="{
                attr: {
                'id': getReCaptchaId() + '-wrapper'
                },
                'afterRender': renderReCaptcha()
                }" id="msp-recaptcha-d84b65fc916e7398083ea5f17089d8f1-wrapper">
                <div class="g-recaptcha" id="msp-recaptcha-d84b65fc916e7398083ea5f17089d8f1">
                    <div style="width: 304px; height: 78px;">
                        <div><iframe
                            src="https://www.google.com/recaptcha/api2/anchor?ar=2&amp;k=6LdBH88UAAAAACG47JS2v7MSqz9BApeP6UJfLcCr&amp;co=aHR0cHM6Ly93d3cua2lja3NjcmV3LmNvbTo0NDM.&amp;hl=en&amp;v=zItNOfzbrqVGbb4QFYpPpcrw&amp;theme=light&amp;size=normal&amp;cb=syu9gixd5g86"
                            width="304" height="78" role="presentation" name="a-34mbugrnmnzj" frameborder="0"
                            scrolling="no"
                            sandbox="allow-forms allow-popups allow-same-origin allow-scripts allow-top-navigation allow-modals allow-popups-to-escape-sandbox"></iframe>
                        </div><textarea id="g-recaptcha-response" name="g-recaptcha-response"
                                class="g-recaptcha-response"
                                style="width: 250px; height: 40px; border: 1px solid rgb(193, 193, 193); margin: 10px 25px; padding: 0px; resize: none; display: none;"></textarea>
                    </div>
                </div>
                <!-- ko if: (getIsInvisibleRecaptcha()) -->
                <div class="field">
                    <div class="control">
                        <input type="checkbox" value="" class="required-captcha checkbox" name="recaptcha-validate-" data-validate="{required:true}">
                    </div>
                </div>
                <!-- /ko -->
                </div>
                <!-- /ko -->
                <!-- /ko -->
                </div>
    
            </fieldset>
            <div class="actions-toolbar">
                <div class="primary">
                    <input type="hidden" name="hideit" id="hideit" value="">
                    <button type="submit" title="Send Message" class="action submit primary">
                        <span>Send Message</span>
                    </button>
                </div>
            </div>
        </form>
        <p><link href="https://use.fontawesome.com/releases/v5.6.3/css/all.css" rel="stylesheet"></p>

    
    <script>
        function openNav() {
            document.getElementById("mySidenav").style.width = "250px";
        }

        function closeNav() {
            document.getElementById("mySidenav").style.width = "0";
        }
    </script>
</body>
</html>